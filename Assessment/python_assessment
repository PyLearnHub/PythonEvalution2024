# Problem 3:

def min_window():
    s = input('').upper()
    t = input('').upper()
    result = ''
    from collections import Counter
    str_data = Counter(s)
    for word in t:
        if word in str_data:
            print(word, end='')
        else:
            None
    return result


value = min_window()
print(value)
