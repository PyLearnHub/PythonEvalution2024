# Problem 1:
def find_strict_attack(*input_list):
    i = 0
    j = 0
    count = 0
    for each_list in input_list:
        if (each_list[i] > each_list[i + 1]) and (each_list[j] > each_list[j + 1]):
            count += 1
            i += 1
            j += 1
        else:
            count = 0
    return count


